<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- Read this before publishing packages to chocolatey.org: https://github.com/chocolatey/chocolatey/wiki/CreatePackages -->
    <id>valgrindparsetools</id>
    <title>ValgrindParseTools (Install)</title>
    <version>1.0.0</version>
    <authors>Gordon Watts</authors>
    <owners>Gordon Watts</owners>
    <summary>Simple tools to parse log files that come from valgrind and produce summaries in Excel files.</summary>
    <description>Parse multiple valgrind log files and produce comparisons, and also produce losses as a function of routine name. Only done for the memcheck tool.
    </description>
    <projectUrl>https://github.com/gordonwatts/MemgrindDifferencer/</projectUrl>
    <tags>valgrindparsetools valgrind memcheck</tags>
    <copyright>Copyright 2015</copyright>
    <licenseUrl>http://choosealicense.com/licenses/mit/</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>First release</releaseNotes>
  </metadata>
  <files>
    <file src="..\memgrind_diff\bin\Release\*" target="tools" exclude="..\memgrind_diff\bin\Release\*vshost*" />
      <file src="..\SimpleDump\bin\Release\FSharp*" target="tools" />
      <file src="..\SimpleDump\bin\Release\FSharp*" target="tools" />
      <file src="..\SimpleDump\bin\Release\SimpleDump.exe" target="tools\memgrind_dump.exe" />
      <file src="..\SimpleDump\bin\Release\SimpleDump.exe.config" target="tools\memgrind_dump.exe.config" />
  </files>
</package>
